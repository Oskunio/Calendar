{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**.csproj"
          ]
        }
      ],
      "dest": "../Documentation/Back",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    },
    {
      "src": [
        {
          "src": "../Calendar",
          "files": [
            "**.csproj"
          ]
        }
      ],
      "dest": "../Documentation/App",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "src": "../Documentation/",
        "files": [
          "Back/*.yml",
          "App/*.yml",
          "Back/index.md"
        ]
      },
      {
        "src": "../Documentation/",
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "src": "../Documentation/",
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "src": "../Documentation/",
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],    
    "dest": "../Documentation/_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": true,
    "disableGitFeatures": false
  }
}